{
  "name": "silkrouter",
  "version": "3.4.5",
  "description": "Silk router is a library for implementing single page applications",
  "main": "dist/umd/index.js",
  "module": "dist/esm/index.esm.js",
  "types": "src/typings/silkrouter.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "start": "node server.js",
    "buildAndStart": "npm run build && npm run start",
    "build": "rollup -c",
    "test": "jest tests/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scssyworks/silkrouter.git"
  },
  "keywords": [
    "router",
    "routing",
    "silk router",
    "SPA",
    "single page applications"
  ],
  "author": "Sachin Singh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scssyworks/silkrouter/issues"
  },
  "homepage": "https://github.com/scssyworks/silkrouter#readme",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@types/jest": "^24.0.19",
    "bootstrap": "^4.3.1",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "jest": "^24.9.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.0",
    "rollup": "^1.25.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^5.1.2"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/js/utils/*.js",
      "src/js/silkrouter.js"
    ],
    "coverageDirectory": "coverage/"
  }
}
