{
  "name": "silkrouter",
  "version": "3.0.0",
  "description": "Silk router is a library for implementing single page applications",
  "main": "dist/js/silkrouter.js",
  "module": "src/js/silkrouter.js",
  "typings": "src/typings/silkrouter.d.ts",
  "scripts": {
    "start": "node server.js",
    "buildAndStart": "npm run build && npm run start",
    "build": "rollup -c",
    "jsdoc": "jsdoc ./src/js/silkrouter.js -p -d docs && jsdoc ./src/js/utils/constants.js -p -d docs && jsdoc ./src/js/utils/helpers.js -p -d docs && jsdoc ./src/js/utils/libs.js -p -d docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scssyworks/silkrouter.git"
  },
  "keywords": [
    "router",
    "routing",
    "silk router",
    "SPA",
    "single page applications"
  ],
  "author": "Sachin Singh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scssyworks/silkrouter/issues"
  },
  "homepage": "https://github.com/scssyworks/silkrouter#readme",
  "peerDependencies": {
    "deparam.js": "^2.0.1",
    "lzstorage": "^1.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "bootstrap": "^4.3.1",
    "deparam.js": "^2.0.1",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "jquery": "^3.4.1",
    "jsdoc": "^3.6.2",
    "lzstorage": "^1.0.8",
    "popper.js": "^1.15.0",
    "rollup": "^1.11.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-uglify": "^6.0.2"
  }
}
